{
	"name": "project-face",
	"version": "2.0.0",
	"private": true,
	"scripts": {
		"dev:admin": "cd admin && npm run dev",
		"dev": "vite dev",
		"dev:backend": "concurrently --kill-others-on-fail \"npm run dev\" \"npm run dev:admin\"",
		"build": "vite build",
		"build:main": "cd admin && npm install && npm run build && cd .. && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"release": "standard-version",
		"release:major": "npm run release -- --release-as major",
		"release:minor": "npm run release -- --release-as minor",
		"release:patch": "npm run release -- --release-as patch"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.6",
		"@commitlint/config-conventional": "^17.6.6",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-vercel": "^3.0.2",
		"@sveltejs/kit": "^1.22.4",
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.2.0",
		"husky": "^8.0.3",
		"postcss": "^8.4.25",
		"standard-version": "^9.5.0",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tailwindcss": "^3.3.2",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"vite-imagetools": "^5.0.5"
	},
	"type": "module",
	"dependencies": {
		"@sanity/vision": "^3.14.2",
		"sanity": "^3.14.2",
		"siema": "^1.5.1",
		"swiper": "^10.1.0"
	}
}
